<?xml version="1.0" encoding="utf-8"?>
<com.insperity.escmobile.view.widget.PunchOrgLevelView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/orgLevelTitleText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/text_grey_dark"
        tools:text="Org Level Name"/>

    <Spinner
        android:id="@+id/orgItemsSpinner"
        style="@style/AppTheme.PunchSpinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="8dp"
        android:padding="2dp"/>
</com.insperity.escmobile.view.widget.PunchOrgLevelView>

